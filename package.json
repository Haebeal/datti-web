{
  "name": "datti-web",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "remix vite:dev",
    "start": "remix-serve ./build/server/index.js",
    "build": "remix vite:build",
    "api:build": "rm -rf ./app/api/datti/* && npx openapi2aspida -o app/api/datti -i https://haebeal.github.io/datti-api/openapi.yaml",
    "lint": "eslint .",
    "format": "prettier --write src/**/*",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
